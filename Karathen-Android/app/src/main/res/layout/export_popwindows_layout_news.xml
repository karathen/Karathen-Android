<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/space_10"
    android:background="@color/wirte"
    android:orientation="horizontal">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@drawable/menu_top_icon_bg_path"
        android:orientation="vertical"
        android:showDividers="middle">

        <RelativeLayout
            android:id="@+id/export_mnemonic_ll"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/space_30"
            android:paddingRight="@dimen/space_10"
            android:layout_marginTop="@dimen/space_20"
            android:visibility="gone">

            <ImageView
                android:id="@+id/zjc_icon"
                android:layout_width="@dimen/space_10"
                android:layout_height="@dimen/space_10"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@drawable/export_mnemonic_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:layout_toRightOf="@+id/zjc_icon"
                android:text="@string/export_mnemonic_string"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />


        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/export_keystore_ll"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/space_30"
            android:paddingRight="@dimen/space_10"
            android:visibility="visible">

            <ImageView
                android:id="@+id/keystore_icon"
                android:layout_width="@dimen/space_10"
                android:layout_height="@dimen/space_10"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@drawable/export_mnemonic_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:layout_toRightOf="@+id/keystore_icon"
                android:text="@string/export_keystore_string"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />


        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/export_private_ll"
            android:layout_width="wrap_content"
            android:paddingRight="@dimen/space_10"
            android:layout_height="@dimen/space_30">

            <ImageView
                android:id="@+id/sy_icon"
                android:layout_width="@dimen/space_10"
                android:layout_height="@dimen/space_10"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@drawable/export_private_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:layout_toRightOf="@+id/sy_icon"
                android:text="@string/export_private_key_string"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />


        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/export_wif_ll"
            android:layout_width="wrap_content"
            android:visibility="gone"
            android:paddingRight="@dimen/space_10"
            android:layout_height="@dimen/space_30">

            <ImageView
                android:id="@+id/wif_icon"
                android:layout_width="@dimen/space_10"
                android:layout_height="@dimen/space_10"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@drawable/export_wif_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:layout_toRightOf="@+id/wif_icon"
                android:text="@string/export_wif_string"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />


        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/update_name_ll"
            android:layout_width="wrap_content"
            android:paddingRight="@dimen/space_10"
            android:layout_height="@dimen/space_30">

            <ImageView
                android:id="@+id/xgmc_icon"
                android:layout_width="@dimen/space_10"
                android:layout_height="@dimen/space_10"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@drawable/update_name_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:layout_toRightOf="@+id/xgmc_icon"
                android:text="@string/update_name_string"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />


        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/browser_query_ll"
            android:layout_width="wrap_content"
            android:paddingRight="@dimen/space_10"
            android:layout_height="@dimen/space_30">

            <ImageView
                android:id="@+id/lyqcx_icon"
                android:layout_width="@dimen/space_10"
                android:layout_height="@dimen/space_10"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@drawable/youlangqi_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:layout_toRightOf="@+id/lyqcx_icon"
                android:text="@string/browser_query_string"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />
        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/delete_ll"
            android:layout_width="wrap_content"
            android:paddingRight="@dimen/space_10"
            android:visibility="gone"
            android:layout_height="@dimen/space_30">

            <ImageView
                android:id="@+id/scdz_icon"
                android:layout_width="@dimen/space_10"
                android:layout_height="@dimen/space_10"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@drawable/delete_address_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:layout_toRightOf="@+id/scdz_icon"
                android:text="@string/delete_address_string"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />


        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/claim_ll"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/space_30"
            android:paddingRight="@dimen/space_10"
            android:visibility="visible">

            <ImageView
                android:id="@+id/tq_icon"
                android:layout_width="@dimen/space_10"
                android:layout_height="@dimen/space_10"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@drawable/get_gas_icon" />

            <TextView
                android:id="@+id/claim_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:layout_toRightOf="@+id/tq_icon"
                android:text="@string/claim_ong_string"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />



        </RelativeLayout>

    </LinearLayout>


</LinearLayout>

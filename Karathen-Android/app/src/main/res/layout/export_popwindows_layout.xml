<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_centerInParent="true"
    android:layout_marginLeft="@dimen/space_32"

    android:orientation="horizontal">

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@color/wirte"
        android:orientation="vertical"
        android:showDividers="middle">

        <LinearLayout
            android:id="@+id/export_mnemonic_ll"
            android:layout_width="122dp"
            android:layout_height="@dimen/space_30"
            android:visibility="gone"
            >

            <ImageView
                android:layout_width="@dimen/space_10"
                android:layout_height="@dimen/space_10"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@drawable/export_mnemonic_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/space_10"
                android:text="@string/export_mnemonic_string"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />


        </LinearLayout>



        <LinearLayout
            android:id="@+id/export_private_ll"
            android:layout_width="122dp"
            android:layout_height="@dimen/space_30"

            >

            <ImageView
                android:layout_width="@dimen/space_10"
                android:layout_height="@dimen/space_10"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@drawable/export_private_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/space_10"
                android:text="@string/export_private_key_string"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />


        </LinearLayout>


        <LinearLayout
            android:id="@+id/export_wif_ll"
            android:layout_width="122dp"
            android:visibility="gone"
            android:layout_height="@dimen/space_30"

            >

            <ImageView
                android:layout_width="@dimen/space_10"
                android:layout_height="@dimen/space_10"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@drawable/export_wif_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/space_10"
                android:text="@string/export_wif_string"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/update_name_ll"
            android:layout_width="122dp"
            android:layout_height="@dimen/space_30">

            <ImageView
                android:layout_width="@dimen/space_10"
                android:layout_height="@dimen/space_10"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@drawable/update_name_icon" />

            <TextView
                android:id="@+id/assets_sign_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/space_10"
                android:text="@string/update_name_string"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/browser_query_ll"
            android:layout_width="122dp"
            android:layout_height="@dimen/space_30">

            <ImageView
                android:layout_width="@dimen/space_10"
                android:layout_height="@dimen/space_10"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@drawable/youlangqi_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/space_10"
                android:text="@string/browser_query_string"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/delete_ll"
            android:layout_width="122dp"
            android:visibility="gone"
            android:layout_height="@dimen/space_30">

            <ImageView
                android:layout_width="@dimen/space_10"
                android:layout_height="@dimen/space_10"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@drawable/delete_address_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/space_10"
                android:text="@string/delete_address_string"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />


        </LinearLayout>


        <LinearLayout
            android:id="@+id/claim_ll"
            android:layout_width="122dp"
            android:visibility="gone"
            android:layout_height="@dimen/space_30">

            <ImageView
                android:layout_width="@dimen/space_10"
                android:layout_height="@dimen/space_10"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@drawable/get_gas_icon" />

            <TextView
                android:id="@+id/claim_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/space_10"
                android:text="@string/claim_ong_string"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />


        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@color/wirte"
        android:orientation="vertical"
        android:showDividers="middle"
        android:visibility="invisible">


        <LinearLayout
            android:layout_width="122dp"
            android:layout_height="@dimen/space_30"

            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/space_10"
                android:text="@string/all_string"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="122dp"
            android:layout_height="@dimen/space_30">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/space_10"
                android:text="ERC-20"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="122dp"
            android:layout_height="@dimen/space_30">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/space_10"
                android:text="ERC-721"
                android:textColor="@color/three_color"
                android:textSize="@dimen/space_10" />


        </LinearLayout>


    </LinearLayout>


</LinearLayout>
